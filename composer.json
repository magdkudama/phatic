{
    "name": "magd-kudama/phatic",
    "description": "Simple and flexible static page generator",
    "keywords": ["PHP", "Static page generator"],
    "homepage": "http://www.magdkudama.com",
    "type": "project",
    "license": "MIT",

    "authors": [
        {
            "name": "Magd Kudama",
            "email": "magdkudama@gmail.com",
            "homepage": "http://www.magdkudama.com"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.3.*@dev",
        "symfony/dependency-injection": "2.4.*@dev",
        "symfony/config": "2.5.*@dev",
        "symfony/yaml": "2.5.*@dev",
        "doctrine/common": "2.5.*@dev",
        "symfony/filesystem": "2.5.*@dev",
        "symfony/finder": "2.5.*@dev",
        "twig/twig": "1.15.*@dev",
        "symfony/event-dispatcher": "2.5.*@dev",
        "symfony/expression-language": "2.5.*@dev"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*@dev",
        "mockery/mockery": "0.8.0"
    },

    "suggest": {
        "magd-kudama/phatic-blog-extension": "Create a blog using Phatic"
    },

    "autoload": {
        "psr-0": {
            "MagdKudama\\Phatic": "src/"
        }
    },

    "bin": ["bin/phatic"],

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}